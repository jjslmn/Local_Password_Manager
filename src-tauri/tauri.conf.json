{
    "productName": "VibeVault",
    "version": "1.0.0",
    "identifier": "com.vibevault.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "vibevault",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": [
            "appimage",
            "deb"
        ],
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/icon.png"
        ]
    }
}